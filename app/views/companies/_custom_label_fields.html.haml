.nested-fields.col-md-12
  .clone-wrap{:style => "border: 1px dashed #999; border-radius: 5px; padding: 20px; margin-bottom: 20px;"}
    .row
      .col-md-4
        .form-group.required
          = f.select :key, options_for_select((::Company.custom_label_options).map{|k| [k, k]}, f.object.key), {include_blank: "Please Select"}, class: 'form-control'
      .col-md-4
        .form-group.required
          = f.text_field :default_value, class: "form-control"
      .col-md-4
        .form-group.required
          = f.text_field :custom_value, class: "form-control"
      .col-md-12
        %center
          = link_to_remove_association "Remove", f, :class=>"btn btn-remove-small"